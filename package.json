{
  "name": "july-28-freedom-portal",
  "version": "1.0.0",
  "private": true,
  "description": "Divine Freedom Portal for JAHmere Webb - July 28th Freedom Movement",
  "author": "Divine Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bravetto/transformation-agents.git"
  },
  "homepage": "https://july28freedom.vercel.app",
  "keywords": [
    "justice",
    "freedom",
    "transformation",
    "divine",
    "portal",
    "jahmere-webb"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo -p 1357",
    "dev:legacy": "next dev -p 1357",
    "build": "next build",
    "build:turbo": "next build --turbo",
    "start": "next start -p 1357",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "clean": "rm -rf .next out",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "performance:audit": "lighthouse http://localhost:3000 --view",
    "performance:ci": "lhci autorun",
    "build:trace": "next build && next internal turbo-trace-server .next/trace-turbopack",
    "verify:optimizations": "node scripts/verify-optimizations.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "docs:health": "node scripts/docs/health-monitor.js",
    "docs:enhanced-health": "node scripts/docs/enhanced-health-monitor.js",
    "docs:validate": "node scripts/docs/validate-documentation.js",
    "docs:fix-links": "node scripts/docs/link-repair.js",
    "docs:maintenance": "npm run docs:enhanced-health && npm run docs:fix-links",
    "docs:emergency-repair": "npm run docs:fix-links && npm run docs:enhanced-health",
    "docs:continuous": "nodemon --watch docs --exec 'npm run docs:enhanced-health'",
    "docs:optimize": "node scripts/docs/content-optimizer.js",
    "docs:ai-suggestions": "node scripts/docs/ai-content-suggestions.js",
    "performance": "node scripts/performance-monitor.js",
    "security-audit": "npm audit && node scripts/security-check.js",
    "agents:certify": "npm run type-check && npm run test && npm run build && npm run docs:enhanced-health",
    "divine:precision": "npm run agents:certify && npm run docs:maintenance && npm run performance",
    "divine:health": "npm run docs:enhanced-health",
    "divine:optimize": "npm run docs:optimize && npm run docs:ai-suggestions",
    "env:setup": "node scripts/environment-setup.js",
    "env:upload": "node scripts/detect-platform-and-upload.js",
    "env:upload:bash": "./scripts/upload-env-to-vercel.sh",
    "env:upload:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/upload-env-to-vercel.ps1",
    "env:pull": "vercel env pull",
    "env:validate": "node scripts/validate-environment.js",
    "env:check": "npm run env:validate && echo 'ðŸ“‹ Environment check complete'",
    "env:help": "node scripts/detect-platform-and-upload.js --help"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.8.1",
    "@types/canvas-confetti": "^1.9.0",
    "@use-gesture/react": "^10.3.1",
    "canvas": "^3.1.2",
    "canvas-confetti": "^1.9.3",
    "chokidar": "^3.5.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "critters": "^0.0.25",
    "date-fns": "^3.3.1",
    "dotenv": "^17.0.1",
    "framer-motion": "^11.0.0",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.300.0",
    "next": "^15.4.2",
    "posthog-js": "^1.257.2",
    "posthog-node": "^5.6.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.60.0",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^2.0.0",
    "web-vitals": "^3.5.2",
    "zod": "^3.25.74",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.4.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.0",
    "lint-staged": "^15.2.0",
    "local-ssl-proxy": "^2.0.5",
    "markdown-link-check": "^3.11.2",
    "msw": "^2.10.3",
    "postcss": "^8.4.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "puppeteer": "^24.12.1",
    "sharp": "^0.34.3",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
